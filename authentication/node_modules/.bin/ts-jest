#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-jest@28.0.1_4db907953e67367985ae85b48dfb7df7/node_modules/ts-jest/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-jest@28.0.1_4db907953e67367985ae85b48dfb7df7/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules:/home/bocanegra/Development/NestJS/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/NestJS/movie/authentication/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-jest@28.0.1_4db907953e67367985ae85b48dfb7df7/node_modules/ts-jest/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-jest@28.0.1_4db907953e67367985ae85b48dfb7df7/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules:/home/bocanegra/Development/NestJS/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/NestJS/movie/authentication/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/ts-jest@28.0.1_4db907953e67367985ae85b48dfb7df7/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/ts-jest@28.0.1_4db907953e67367985ae85b48dfb7df7/node_modules/ts-jest/cli.js" "$@"
fi
