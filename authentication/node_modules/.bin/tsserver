#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/typescript@4.6.4/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules:/home/bocanegra/Development/NestJS/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/NestJS/movie/authentication/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/typescript@4.6.4/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules:/home/bocanegra/Development/NestJS/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/NestJS/movie/authentication/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/tsserver" "$@"
fi
