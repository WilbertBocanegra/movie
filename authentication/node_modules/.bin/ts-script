#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules/ts-node/dist/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules/ts-node/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules:/home/bocanegra/Development/NestJS/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/NestJS/movie/authentication/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules/ts-node/dist/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules/ts-node/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules/.pnpm/node_modules:/home/bocanegra/Development/NestJS/movie/node_modules:/home/bocanegra/Development/NestJS/node_modules:/home/bocanegra/Development/node_modules:/home/bocanegra/node_modules:/home/node_modules:/node_modules:/home/bocanegra/Development/NestJS/movie/authentication/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/ts-node@10.7.0_fd4b5ff48b408bf425abfec4a641da13/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
fi
